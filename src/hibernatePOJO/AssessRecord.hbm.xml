<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.AssessRecord" table="assess_record" schema="test">
        <id name="aid">
            <column name="aid" sql-type="int(11)"/>
        </id>
        <property name="did">
            <column name="did" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="degree">
            <column name="degree" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="teid">
            <column name="teid" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="eventclass">
            <column name="eventclass" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="time">
            <column name="time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>