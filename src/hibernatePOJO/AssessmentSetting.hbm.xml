<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.AssessmentSetting" table="assessment_setting" schema="test">
        <id name="aid">
            <column name="aid" sql-type="varchar(255)"/>
        </id>
        <property name="redyellow">
            <column name="redyellow" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="yellowgreen">
            <column name="yellowgreen" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>