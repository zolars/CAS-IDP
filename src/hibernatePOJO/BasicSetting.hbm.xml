<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.BasicSetting" table="basic_setting" schema="test">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="onlineinterval">
            <column name="onlineinterval" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="thansentinterval">
            <column name="thansentinterval" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="uploadinterval">
            <column name="uploadinterval" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>