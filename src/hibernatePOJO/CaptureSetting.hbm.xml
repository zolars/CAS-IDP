<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.CaptureSetting" table="capture_setting" schema="test">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="ip">
            <column name="ip" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="port1">
            <column name="port1" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="port2">
            <column name="port2" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="onlineinterval">
            <column name="onlineinterval" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="thansentinterval">
            <column name="thansentinterval" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="uploadinterval">
            <column name="uploadinterval" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="mpid">
            <column name="mpid" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>