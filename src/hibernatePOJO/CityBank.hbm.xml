<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.CityBank" table="city_bank" schema="test">
        <id name="cbid">
            <column name="cbid" sql-type="varchar(255)"/>
        </id>
        <property name="cbname">
            <column name="cbname" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="compRoom">
            <column name="compRoom" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>