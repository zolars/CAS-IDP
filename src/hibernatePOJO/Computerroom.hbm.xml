<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.Computerroom" table="computerroom" schema="test">
        <id name="rid">
            <column name="rid" sql-type="varchar(255)"/>
        </id>
        <property name="rname">
            <column name="rname" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="didset">
            <column name="didset" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="tempset">
            <column name="tempset" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="cidset">
            <column name="cidset" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>