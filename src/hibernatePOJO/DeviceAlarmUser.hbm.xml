<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.DeviceAlarmUser" table="device_alarm_user" schema="test">
        <composite-id mapped="true" class="hibernatePOJO.DeviceAlarmUserPK">
            <key-property name="id">
                <column name="id" sql-type="varchar(255)"/>
            </key-property>
            <key-property name="aid">
                <column name="aid" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="timeperiod">
            <column name="timeperiod" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="isAlert">
            <column name="isAlert" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isPlantform">
            <column name="isPlantform" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isSms">
            <column name="isSMS" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="precontent">
            <column name="precontent" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="uid">
            <column name="uid" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="did">
            <column name="did" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>