<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.DeviceAlarmUser" table="device_alarm_user" schema="test">
        <id name="id">
            <column name="id" sql-type="varchar(255)"/>
        </id>
        <property name="did">
            <column name="did" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="uid">
            <column name="uid" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="stime">
            <column name="stime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="etime">
            <column name="etime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="precontent">
            <column name="precontent" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>