<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.Dictionary" table="dictionary" schema="test">
        <id name="id">
            <column name="id" sql-type="int(255)"/>
        </id>
        <property name="item">
            <column name="item" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="coefficient">
            <column name="coefficient" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>