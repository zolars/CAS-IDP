<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.DictionaryPlus" table="dictionary_plus" schema="test">
        <id name="part">
            <column name="part" sql-type="int(255)"/>
        </id>
        <property name="slaveid">
            <column name="slaveid" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="functioncode">
            <column name="functioncode" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="start">
            <column name="start" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="length">
            <column name="length" sql-type="int(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>