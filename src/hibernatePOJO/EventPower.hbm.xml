<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.EventPower" table="event_power" schema="test">
        <id name="teid">
            <column name="teid" sql-type="int(11)"/>
        </id>
        <property name="cid">
            <column name="cid" sql-type="int(11)"/>
        </property>
        <property name="value">
            <column name="value" sql-type="double" precision="-1"/>
        </property>
        <property name="limit">
            <column name="limit" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="did">
            <column name="did" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="time">
            <column name="time" sql-type="datetime"/>
        </property>
        <property name="duration">
            <column name="duration" sql-type="int(11)"/>
        </property>
        <property name="signature">
            <column name="signature" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="annotation">
            <column name="annotation" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>