<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.EventTransient" table="event_transient" schema="test">
        <id name="teid">
            <column name="teid" sql-type="int(11)"/>
            <generator class="increment"></generator>
        </id>
        <property name="mpid">
            <column name="mpid" sql-type="int(11)"/>
        </property>
        <property name="time">
            <column name="time" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="duration">
            <column name="duration" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="value">
            <column name="value" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="subtype">
            <column name="subtype" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>