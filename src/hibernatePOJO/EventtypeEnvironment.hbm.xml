<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.EventtypeEnvironment" table="eventtype_environment" schema="test">
        <id name="eid">
            <column name="eid" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="priorty">
            <column name="priorty" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>