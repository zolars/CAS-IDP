<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.Smsplantlevel" table="smsplantlevel" schema="test">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="plantlevel">
            <column name="plantlevel" sql-type="int(11)"/>
        </property>
        <property name="levelbz">
            <column name="levelbz" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="syslevel">
            <column name="syslevel" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>