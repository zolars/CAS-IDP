<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.Computerroom" table="computerroom" schema="Test">
        <id name="rid">
            <column name="rid" sql-type="varchar(255)"/>
        </id>
        <property name="rname">
            <column name="rname" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="tempratureMain">
            <column name="temprature_main" sql-type="float(255)" precision="255" not-null="true"/>
        </property>
        <property name="tempratureSupport">
            <column name="temprature_support" sql-type="float(255)" precision="255" not-null="true"/>
        </property>
        <property name="tempratureAssist">
            <column name="temprature_assist" sql-type="float(255)" precision="255" not-null="true"/>
        </property>
        <property name="humidityMain">
            <column name="humidity_main" sql-type="float(255)" precision="255" not-null="true"/>
        </property>
        <property name="humiditySupport">
            <column name="humidity_support" sql-type="float(255)" precision="255" not-null="true"/>
        </property>
        <property name="humidityAssist">
            <column name="humidity_assist" sql-type="float(255)" precision="255" not-null="true"/>
        </property>
        <property name="upsset">
            <column name="upsset" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="idp">
            <column name="idp" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="didset">
            <column name="didset" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="mpidset">
            <column name="mpidset" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>