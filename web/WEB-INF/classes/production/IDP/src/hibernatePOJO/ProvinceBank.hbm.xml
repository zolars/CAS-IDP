<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernatePOJO.ProvinceBank" table="province_bank" schema="Test">
        <id name="pbid">
            <column name="pbid" sql-type="varchar(255)"/>
        </id>
        <property name="pbname">
            <column name="pbname" sql-type="varchar(255)"/>
        </property>
        <property name="cbidset">
            <column name="cbidset" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>